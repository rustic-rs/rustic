{
    "lineWidth": 80,
    "markdown": {
        "lineWidth": 80,
        "emphasisKind": "asterisks",
        "strongKind": "asterisks",
        "textWrap": "always"
    },
    "toml": {
        "lineWidth": 80
    },
    "includes": [
        "**/*.{md}",
        "**/*.{toml}"
    ],
    "excludes": [],
    "plugins": [
        "https://plugins.dprint.dev/markdown-0.15.2.wasm",
        "https://plugins.dprint.dev/toml-0.5.4.wasm"
    ]
}